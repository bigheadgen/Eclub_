/**
 * 
 */
package br.com.solar.model;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * @author Endrigo
 *
 */
@XmlRootElement
public class Turma {
	int id;
	String nome;
	
	public Turma() {
	}
	
	public Turma(int id, String nome){
		this.id = id;
		this.nome = nome;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}
	
	

}
